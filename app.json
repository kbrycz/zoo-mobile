{
  "expo": {
    "name": "Brycz Zoo",
    "slug": "zoo-mobile",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/main/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2c3e50"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Giving Brycz Zoo access to your photos allows you to add images to your profile."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kbrycz2555.bryczzoo",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Giving Brycz Zoo access to your photos allows you to add images to your profile."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2c3e50"
      },
      "package": "com.kbrycz2555.bryczzoo",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7014a336-5f47-4c52-9baa-d91771736577"
      }
    }
  }
}
